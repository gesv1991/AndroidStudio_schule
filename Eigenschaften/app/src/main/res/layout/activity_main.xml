<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.sveng.eigenschaften.MainActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/textView18"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginEnd="34dp"
            android:layout_marginTop="97dp"
            android:layout_toStartOf="@+id/tvDensity"
            android:text="Density" />

        <TextView
            android:id="@+id/textView19"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/textView18"
            android:layout_below="@+id/textView18"
            android:layout_marginTop="17dp"
            android:text="Density DPI" />

        <TextView
            android:id="@+id/textView20"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/textView19"
            android:layout_below="@+id/textView19"
            android:layout_marginTop="18dp"
            android:text="Height px" />

        <TextView
            android:id="@+id/textView21"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/textView20"
            android:layout_below="@+id/textView20"
            android:layout_marginTop="18dp"
            android:text="Width px" />

        <TextView
            android:id="@+id/textView22"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/textView21"
            android:layout_below="@+id/textView21"
            android:layout_marginTop="33dp"
            android:text="Locale" />

        <TextView
            android:id="@+id/textView23"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/textView22"
            android:layout_below="@+id/textView22"
            android:layout_marginTop="21dp"
            android:text="Version SDK" />

        <TextView
            android:id="@+id/textView24"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/textView23"
            android:layout_below="@+id/textView23"
            android:layout_marginTop="18dp"
            android:text="Release" />

        <TextView
            android:id="@+id/tvDensity"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/textView19"
            android:layout_centerHorizontal="true"
            android:text="TextView" />

        <TextView
            android:id="@+id/tvDensityDPI"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/textView20"
            android:layout_alignEnd="@+id/tvDensity"
            android:text="TextView" />

        <TextView
            android:id="@+id/tvHeightPx"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/tvDensityDPI"
            android:layout_alignTop="@+id/textView20"
            android:text="TextView" />

        <TextView
            android:id="@+id/tvWidthPx"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/tvHeightPx"
            android:layout_alignTop="@+id/textView21"
            android:text="TextView" />

        <TextView
            android:id="@+id/tvLocale"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/textView22"
            android:layout_alignBottom="@+id/textView22"
            android:layout_alignStart="@+id/tvWidthPx"
            android:text="TextView" />

        <TextView
            android:id="@+id/tvVersionSdk"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/tvLocale"
            android:layout_alignTop="@+id/textView23"
            android:text="TextView" />

        <TextView
            android:id="@+id/tvRelease"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/textView24"
            android:layout_centerHorizontal="true"
            android:text="TextView" />
    </RelativeLayout>
</LinearLayout>
